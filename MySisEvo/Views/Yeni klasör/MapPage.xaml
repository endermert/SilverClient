                                                <DataTemplate>
                                                    <TextBlock Margin="2" Text="{Binding Data.DT_KONTAK}" Foreground="Navy" />
                                                </DataTemplate>
                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>

