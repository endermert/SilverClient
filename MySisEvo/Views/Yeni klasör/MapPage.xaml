           Title="Harita | Mysis Uzaktan Okuma Sistemleri"
           Style="{StaticResource PageStyle}" SizeChanged="SizeChanged1" Loaded="Page_Loaded"> 
    
    <UserControl.Resources>
        <Style x:Key="defaultLineStyle" TargetType="telerik:MapShape">
            <Setter Property="StrokeThickness" Value="5" />
            <Setter Property="Stroke" Value="Transparent" />
        </Style>
        <Style x:Key="selectedLineStyle" TargetType="telerik:MapShape">
            <Setter Property="StrokeThickness" Value="5" />
            <Setter Property="Stroke" Value="Green" />
        </Style>
        <Style x:Key="defaultPolylineStyle" TargetType="telerik:MapShape">
            <Setter Property="StrokeThickness" Value="15" />
            <Setter Property="Stroke" Value="Blue" />
            <Setter Property="Opacity" Value="0.3"/>
        </Style>
        <Style x:Key="mavi" TargetType="telerik:MapShape">
            <Setter Property="StrokeThickness" Value="5" />
            <Setter Property="Stroke" Value="Blue" />
            <Setter Property="Opacity" Value="0.5"/>
        </Style>
        
        <Style x:Key="yesil" TargetType="telerik:MapShape">
            <Setter Property="StrokeThickness" Value="5" />
            <Setter Property="Stroke" Value="Red" />
            <Setter Property="Opacity" Value="0.5"/>
        </Style>
        <Style x:Key="selectedPolylineStyle" TargetType="telerik:MapShape">
            <Setter Property="StrokeThickness" Value="15" />
            <Setter Property="Stroke" Value="Red" />
        </Style>
        <data:Entities x:Key="EntityObject" />
        <Storyboard x:Name="myStoryboard">
            <DoubleAnimation  x:Name="myDoubleAni"
            Storyboard.TargetProperty="Opacity"
            From="1.0" To="0.0" Duration="0:0:0.5" 
            AutoReverse="True" RepeatBehavior="Forever" />
        </Storyboard>
        
    </UserControl.Resources>
