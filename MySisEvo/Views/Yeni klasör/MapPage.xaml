                                                                           Width="450" Height="400"
                                                                           RadiusX="10" RadiusY="10">
                                                                   </Rectangle>

                                                                   <!-- main rect -->
                                                               <Rectangle Width="450" Height="400"  RadiusX="10" RadiusY="10" Opacity="0.5">
                                                                        <Rectangle.Fill>
                                                                            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                                                                <GradientStop Offset="0" Color="DarkOliveGreen"/>
                                                                                <GradientStop Offset="1" Color="DarkOliveGreen"/>
                                                                            </LinearGradientBrush>
                                                                        </Rectangle.Fill>
                                                                   </Rectangle>

                                                                   <!-- transparent rect -->
                                                                   <Rectangle Width="448" Height="398" RadiusX="10" RadiusY="10">
                                                                        <Rectangle.Fill>
                                                                            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                                                                <GradientStop Offset="0" Color="DarkOliveGreen"/>
                                                                                <GradientStop Offset="1" Color="#00000000"/>
                                                                            </LinearGradientBrush>
                                                                        </Rectangle.Fill>
                                                                    </Rectangle>

