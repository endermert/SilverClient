                                                                                    MinWidth="120">
                                                                                </telerik:RadNumericUpDown>

                                                                            </StackPanel>
                                                                            <telerik:RadButton Click="Button_Click" Width="80" Height="35" Margin="5" >
                                                                                <telerik:RadButton.Content>
                                                                                    <StackPanel Orientation="Horizontal">
                                                                                        <Image Source="/MySisEvo;component/Images/deny.png" Height="32" Width="32" />
                                                                                        <TextBlock Text="KAPAT" Foreground="Navy" Margin="5,7,0,0" />
                                                                                    </StackPanel>
                                                                                </telerik:RadButton.Content>
                                                                            </telerik:RadButton>


                                                                        </StackPanel>
                                                                    </Border>
                                                                    
                                                                        <Border BorderThickness="2" CornerRadius="5" Margin="5" BorderBrush="White" HorizontalAlignment="Right">
                                                                            <Image Width="120" Stretch="Uniform" Margin="5" Source="{Binding Path=Arac.ARC_RES1M}">
                                                                                <Image.Effect>
                                                                                    <DropShadowEffect Color="Black" Direction="315" Opacity="0.8" ShadowDepth="7"/>
                                                                                </Image.Effect>
                                                                            </Image>
                                                                        </Border>
                                                                        
                                                                    

                                                                </StackPanel>
                                                                <StackPanel Orientation="Horizontal" Width="420" Margin="-25,0,0,0">
                                                                    <Border BorderThickness="2" CornerRadius="5" BorderBrush="White" Margin="2">

                                                                        <StackPanel Orientation="Vertical" Width="190" Margin="5">
                                                                                <StackPanel Orientation="Horizontal" Margin="5" >
                                                                                    <TextBlock Text="V1" Width="50"  FontSize="15" Foreground="White" FontWeight="Bold" />
                                                                                    <TextBlock Text=":" FontSize="15" Foreground="White" FontWeight="Bold" />
                                                                                    <TextBlock Text="280 volt" FontSize="15" Foreground="White" FontWeight="Bold" />
                                                                                </StackPanel>
                                                                                <StackPanel Orientation="Horizontal" Margin="5">
                                                                                    <TextBlock Text="V2" Width="50" FontSize="15" Foreground="White" FontWeight="Bold"/>
                                                                                    <TextBlock Text=":" FontSize="15" Foreground="White" FontWeight="Bold"/>
                                                                                    <TextBlock Text="220 volt" FontSize="15" Foreground="White" FontWeight="Bold"/>
                                                                                </StackPanel>
                                                                                <StackPanel Orientation="Horizontal" Margin="5">
                                                                                    <TextBlock Text="Akım" Width="50" FontSize="15" Foreground="White" FontWeight="Bold"/>
