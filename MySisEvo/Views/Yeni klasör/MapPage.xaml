                                                                                        </StackPanel>
                                                                                    </telerik:RadButton.Content>
                                                                                </telerik:RadButton>
                                                                                <telerik:RadButton  Width="120" Height="35" Margin="5" >
                                                                                    <telerik:RadButton.Content>
                                                                                        <StackPanel Orientation="Horizontal">
                                                                                            <Image Source="/MySisEvo;component/Images/add.png" Height="32" Width="32" />
                                                                                            <TextBlock Text="KATOTiK OKU" Foreground="Navy" Margin="5,7,0,0" />
                                                                                        </StackPanel>
                                                                                    </telerik:RadButton.Content>
                                                                                </telerik:RadButton>
                                                                               
                                                                            </StackPanel>
                                                                            
                                                                            <StackPanel Orientation="Horizontal" >
                                                                                <telerik:RadButton  Width="120" Height="35" Margin="5" >
                                                                                    <telerik:RadButton.Content>
                                                                                        <StackPanel Orientation="Horizontal">
                                                                                            <Image Source="/MySisEvo;component/Images/add.png" Height="32" Width="32" />
                                                                                            <TextBlock Text="KATOTiK YAZ" Foreground="Navy" Margin="5,7,0,0" />
                                                                                        </StackPanel>
                                                                                    </telerik:RadButton.Content>
                                                                                </telerik:RadButton>
